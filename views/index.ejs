<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <!-- WRAPPER -->
    <div id="wrapper">
      <div id="userInfo">
        <h2>User Info</h2>
        <p>
          <strong>Name: <span id="userInfoName"></span></strong><br>
          <strong>Age: <span id="userInfoAge"></span></strong><br>
          <strong>Gender: <span id="userInfoGender"></span></strong><br>
          <strong>Location: <span id="userInfoLocation"></span></strong><br>
        </p>
      </div>
      <!-- USER LIST -->
      <h2>User List</h2>
      <div id="userList">
        <table>
          <thead>
            <th>UserName</th>
            <th>Email</th>
            <th>Delete ?</th>
          </thead>
          <tbody>
          </tbody>
        </table>
      </div>
      <!-- /USER LIST -->
    </div>
    <!-- /WRAPPER -->
    <script defer src='/javascripts/global.js'></script>
  </body>
</html>
